@prefix ns1: <http://www.example.com/genealogy.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.co-ode.org/roberts/family-tree.owl> a owl:Ontology ;
    rdfs:comment "A simple family relationships ontology and associated instances. the description is of the family of Robert Stevens and the intention is to use the minimal of asserted relationships and the maximum of inference. To do this  I've used role chains, nominals and properties hierarchies." .

<http://www.co-ode.org/roberts/family-tree.owl#hasBirthYear> a rdfs:Datatype .

ns1:Ancestor a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( ns1:Person [ a owl:Restriction ;
                        owl:onProperty ns1:isAncestorOf ;
                        owl:someValuesFrom ns1:Person ] ) ] .

ns1:alsoKnownAs a owl:AnnotationProperty .

ns1:formerlyKnownAs a owl:AnnotationProperty .

ns1:hasBirthYear a owl:AnnotationProperty .

ns1:hasBrother a owl:ObjectProperty ;
    owl:inverseOf ns1:isBrotherOf .

ns1:hasDaughter a owl:ObjectProperty ;
    rdfs:subPropertyOf ns1:hasChild ;
    owl:inverseOf ns1:isDaughterOf .

ns1:hasDeathYear a owl:AnnotationProperty .

ns1:hasHusband a owl:ObjectProperty ;
    rdfs:range ns1:Man ;
    rdfs:subPropertyOf ns1:hasSpouse ;
    owl:inverseOf ns1:isHusbandOf ;
    owl:propertyChainAxiom ( ns1:isFemalePartnerIn ns1:hasMalePartner ) .

ns1:hasMarriageYear a owl:AnnotationProperty .

ns1:hasSister a owl:ObjectProperty ;
    owl:inverseOf ns1:isSisterOf .

ns1:hasSon a owl:ObjectProperty ;
    rdfs:subPropertyOf ns1:hasChild ;
    owl:inverseOf ns1:isSonOf .

ns1:hasWife a owl:ObjectProperty ;
    rdfs:range ns1:Woman ;
    rdfs:subPropertyOf ns1:hasSpouse ;
    owl:inverseOf ns1:isWifeOf ;
    owl:propertyChainAxiom ( ns1:isMalePartnerIn ns1:hasFemalePartner ) .

ns1:isUncleOf a owl:ObjectProperty ;
    rdfs:domain ns1:Man ;
    rdfs:range ns1:Person ;
    owl:propertyChainAxiom ( ns1:isBrotherOf ns1:isParentOf ) .

ns1:knownAs a owl:AnnotationProperty .

ns1:m151 ns1:hasFemalePartner ns1:charlotte_hewett_1863 .

ns1:m171 ns1:hasMalePartner ns1:thomas_burton_1765 .

ns1:hasAncestor a owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:subPropertyOf ns1:hasRelation ;
    owl:inverseOf ns1:isAncestorOf .

ns1:hasFather a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain ns1:Person ;
    rdfs:range ns1:Man ;
    rdfs:subPropertyOf ns1:hasParent ;
    owl:inverseOf ns1:isFatherOf .

ns1:hasFemalePartner a owl:ObjectProperty ;
    rdfs:domain ns1:Marriage ;
    rdfs:range ns1:Woman ;
    rdfs:subPropertyOf ns1:hasPartner ;
    owl:inverseOf ns1:isFemalePartnerIn .

ns1:hasMalePartner a owl:ObjectProperty ;
    rdfs:domain ns1:Marriage ;
    rdfs:range ns1:Man ;
    rdfs:subPropertyOf ns1:hasPartner ;
    owl:inverseOf ns1:isMalePartnerIn .

ns1:hasMother a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain ns1:Person ;
    rdfs:range ns1:Woman ;
    rdfs:subPropertyOf ns1:hasParent ;
    owl:inverseOf ns1:isMotherOf .

ns1:isBloodrelationOf a owl:ObjectProperty ;
    rdfs:subPropertyOf ns1:hasRelation .

ns1:isDaughterOf a owl:ObjectProperty ;
    rdfs:subPropertyOf ns1:isChildOf .

ns1:isFatherOf a owl:ObjectProperty .

ns1:isHusbandOf a owl:ObjectProperty .

ns1:isMotherOf a owl:ObjectProperty .

ns1:isPartnerIn a owl:ObjectProperty .

ns1:isSisterOf a owl:ObjectProperty ;
    rdfs:domain ns1:Woman ;
    rdfs:range ns1:Person ;
    rdfs:subPropertyOf ns1:isSiblingOf .

ns1:isSonOf a owl:ObjectProperty ;
    rdfs:subPropertyOf ns1:isChildOf .

ns1:isSpouseOf a owl:ObjectProperty .

ns1:isWifeOf a owl:ObjectProperty .

ns1:james_hewett_1815 ns1:isFatherOf ns1:charlotte_hewett_1863,
        ns1:elisa_amelia_hewett_1858,
        ns1:emma_hewett_1860,
        ns1:james_hewett_1850,
        ns1:jane_hewett_1849,
        ns1:john_james_hewett_1845,
        ns1:mary_ann_hewett_1852,
        ns1:rosena_martha_hewett_1847 .

ns1:m172 a owl:NamedIndividual ;
    ns1:hasFemalePartner ns1:jane_burton_1823 ;
    ns1:hasMalePartner ns1:james_hewett_1815 ;
    ns1:hasMarriageYear 1845 .

ns1:Female a owl:Class ;
    rdfs:subClassOf ns1:Sex ;
    owl:disjointWith ns1:Male .

ns1:hasChild a owl:ObjectProperty ;
    owl:inverseOf ns1:isChildOf .

ns1:hasPartner a owl:ObjectProperty ;
    rdfs:domain ns1:Marriage ;
    rdfs:range ns1:Person ;
    owl:inverseOf ns1:isPartnerIn .

ns1:hasRelation a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:domain ns1:Person ;
    rdfs:range ns1:Person .

ns1:hasSpouse a owl:ObjectProperty ;
    owl:inverseOf ns1:isSpouseOf .

ns1:isAncestorOf a owl:ObjectProperty .

ns1:isBrotherOf a owl:ObjectProperty ;
    rdfs:domain ns1:Man ;
    rdfs:range ns1:Person ;
    rdfs:subPropertyOf ns1:isSiblingOf .

ns1:isFemalePartnerIn a owl:ObjectProperty .

ns1:isMalePartnerIn a owl:ObjectProperty .

ns1:isSiblingOf a owl:ObjectProperty,
        owl:SymmetricProperty,
        owl:TransitiveProperty ;
    rdfs:subPropertyOf ns1:isBloodrelationOf ;
    owl:propertyChainAxiom ( ns1:hasParent ns1:isParentOf ) .

ns1:thomas_burton_1765 a owl:NamedIndividual ;
    ns1:hasBirthYear 1765 ;
    ns1:isFatherOf ns1:bethia_burton_1821,
        ns1:jane_burton_1823,
        ns1:martha_taylor_burton_1816,
        ns1:thomas_burton_1826 .

ns1:DomainEntity a owl:Class .

ns1:Male a owl:Class ;
    rdfs:subClassOf ns1:Sex .

ns1:Marriage a owl:Class ;
    rdfs:subClassOf ns1:DomainEntity .

ns1:hasSex a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain ns1:Person ;
    rdfs:range ns1:Sex .

ns1:isParentOf a owl:ObjectProperty .

ns1:james_hewett_1850 a owl:NamedIndividual ;
    ns1:hasBirthYear 1850 ;
    ns1:isBrotherOf ns1:mary_ann_hewett_1852 .

ns1:jane_burton_1823 a owl:NamedIndividual ;
    ns1:hasBirthYear 1823 ;
    ns1:isMotherOf ns1:charlotte_hewett_1863,
        ns1:elisa_amelia_hewett_1858,
        ns1:emma_hewett_1860,
        ns1:james_hewett_1850,
        ns1:jane_hewett_1849,
        ns1:john_james_hewett_1845,
        ns1:mary_ann_hewett_1852,
        ns1:rosena_martha_hewett_1847 .

ns1:charlotte_hewett_1863 a owl:NamedIndividual ;
    ns1:hasBirthYear 1863 ;
    ns1:hasDeathYear 1940 ;
    ns1:isMotherOf ns1:charlotte_caroline_jane_bright_1894,
        ns1:frederick_herbert_bright_1889,
        ns1:henry_edmund_bright_1887,
        ns1:james_frank_hayden_bright_1891,
        ns1:kathleen_minnie_bright_1904,
        ns1:leonard_john_bright_1890,
        ns1:nora_ada_bright_1899,
        ns1:william_george_bright_1901 .

ns1:elisa_amelia_hewett_1858 a owl:NamedIndividual ;
    ns1:hasBirthYear 1858 ;
    ns1:isSisterOf ns1:emma_hewett_1860 .

ns1:emma_hewett_1860 a owl:NamedIndividual ;
    ns1:hasBirthYear 1860 ;
    ns1:isSisterOf ns1:elisa_amelia_hewett_1858 .

ns1:hasParent a owl:ObjectProperty ;
    rdfs:domain ns1:Person ;
    rdfs:range ns1:Person ;
    rdfs:subPropertyOf ns1:hasAncestor ;
    owl:equivalentProperty ns1:isChildOf ;
    owl:inverseOf ns1:isParentOf .

ns1:isChildOf a owl:ObjectProperty .

ns1:jane_hewett_1849 a owl:NamedIndividual ;
    ns1:hasBirthYear 1849 ;
    ns1:isSisterOf ns1:john_james_hewett_1845 .

ns1:john_james_hewett_1845 a owl:NamedIndividual ;
    ns1:hasBirthYear 1845 ;
    ns1:isBrotherOf ns1:rosena_martha_hewett_1847 .

ns1:rosena_martha_hewett_1847 a owl:NamedIndividual ;
    ns1:hasBirthYear 1847 ;
    ns1:isSisterOf ns1:mary_ann_hewett_1852 .

ns1:Sex a owl:Class ;
    rdfs:subClassOf ns1:DomainEntity ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( ns1:Female ns1:Male ) ] .

ns1:mary_ann_hewett_1852 a owl:NamedIndividual ;
    ns1:hasBirthYear 1852 ;
    ns1:isSisterOf ns1:jane_hewett_1849 .

ns1:Woman a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( ns1:Person [ a owl:Restriction ;
                        owl:onProperty ns1:hasSex ;
                        owl:someValuesFrom ns1:Female ] ) ] .

ns1:Man a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( ns1:Person [ a owl:Restriction ;
                        owl:onProperty ns1:hasSex ;
                        owl:someValuesFrom ns1:Male ] ) ] .

ns1:Person a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:hasMother ;
            owl:someValuesFrom ns1:Woman ],
        [ a owl:Restriction ;
            owl:onProperty ns1:hasSex ;
            owl:someValuesFrom ns1:Sex ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
            owl:onClass ns1:Person ;
            owl:onProperty ns1:hasParent ],
        [ a owl:Restriction ;
            owl:onProperty ns1:hasFather ;
            owl:someValuesFrom ns1:Man ],
        ns1:DomainEntity ;
    owl:disjointWith ns1:Sex ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( ns1:Man ns1:Woman ) ] .

